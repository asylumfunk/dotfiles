#!/bin/sh

alias ga='git add'
alias gb='git branch'
alias gba='git branch --all'
alias gbd='git branch --delete'
alias gc='git commit'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gd='git diff'
alias gdc='git diff --cached'
alias gdm='git diff master'
alias gl='git log'
alias gll='git log --all --decorate --graph --pretty=medium -p'
alias gm='git merge'
alias gp='git pull'
alias gps='git push'
alias gs='git status -sb'
alias gsh='git show  '
alias gst='git stash'
alias gt='git tag'
alias gw='git whatchanged'

